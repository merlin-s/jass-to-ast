    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voces/jass-to-ast.git"
  },
  "keywords": [
    "wc3",
    "jass",
    "webcraft",
    "warcraft",
    "ast"
  ],
  "author": "verit",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/voces/jass-to-ast/issues"
  },
  "homepage": "https://github.com/voces/jass-to-ast#readme",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^26.0.14",
    "@types/nearley": "^2.11.1",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "eslint": "^7.10.0",
    "eslint-config-verit": "^3.2.1",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@voces/nearley": "^2.16.0",
    "moo": "^0.5.1"
  },
  "type": "module",
  "files": [
    "/src",
    "index.js"
  ],
  "bin": {
    "jass-to-ast": "index.js"
  }
