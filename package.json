  "description": "Converts WC3 JASS to an abstract syntax tree",
  "main": "src/parser.js",
  "module": "src/parser.js",
  "scripts": {
    "build": "node --experimental-modules --no-warnings src/grammar/build.js",
    "lint": "eslint src --ext .js,.ts --fix",
